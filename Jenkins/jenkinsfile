pipeline { 

    environment { 

        registry = "YourDockerhubAccount/YourRepository" 

        registryCredential = 'dockerhub_id' 

        dockerImage = '' 

    }

    agent any 

    stages { 

        stage('Cloning our Git') { 

            steps { 

                git 'https://github.com/YourGithubAccount/YourGithubRepository.git' 

            }

        }
